    {
      "parameters": {
        "jsCode": "// Tomamos todos los workflows del nodo \"Preparar Datos\"\nconst workflows = $items(\"Preparar Datos\");\n\n// Tomamos todos los archivos del nodo \"Buscar duplicado por ID\"\nconst repoFiles = $items(\"Buscar duplicado por ID\");\n\nconst results = [];\n\nfor (const w of workflows) {\n  const wf = w.json;\n  const match = repoFiles.find(f => {\n    const name = f.json.name;\n    return name && name.startsWith(wf.workflow_id + \"_\");\n  });\n\n  if (match) {\n    results.push({\n      json: {\n        ...wf,\n        file_found: true,\n        oldFileName: match.json.name,\n        oldFilePath: match.json.path,\n        sha: match.json.sha,\n      },\n    });\n  } else {\n    results.push({\n      json: {\n        ...wf,\n        file_found: false,\n      },\n    });\n  }\n}\n\nreturn results;\n"
      },
